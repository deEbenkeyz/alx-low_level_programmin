#include "main.h"

/**
 * print_to_98 - main function
 * @n: value to be checked
 * 
 */
void print_to_98(int n)
{
	while (n >= 98)
	{
		_putchar(n);

		if (n != 98)
		{
			_putchar(',');
			_putchar(' ');
		}
		n--;
	}
	
	while (n <= 98)
	{
		_putchar(n);
		if (n != 98)
		{
			_putchar(',');
			_putchar(' ');
		}
		n++;
	}

	while (n == 98)
	{
	  _putchar(n);
	}
	_putchar('\n');
}
